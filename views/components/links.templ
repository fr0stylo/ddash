package components

templ LinkItem(label string, url string) {
	<a class="flex items-center justify-between rounded-lg border border-gray-200 bg-white px-3 py-2 text-sm text-gray-700 hover:bg-gray-50" href={ url } target="_blank" rel="noreferrer">
		<span>{ label }</span>
		<span class="text-xs text-gray-400">Open</span>
	</a>
}
